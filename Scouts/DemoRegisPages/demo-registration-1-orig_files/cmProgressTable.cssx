.cmProgressTableWrapper {
	width:299px;
	border:1px solid #ccc;
	padding:1px;
	margin:20px auto;}

table.cmProgressTable {
	background:#fff;
	border-collapse:collapse;}

table.cmProgressTable td {
	padding:0;}

table.cmProgressTable td div {
	width:2px;
	height:36px;
	background-color:#1eb036;
	border-right:1px solid #fff;}
